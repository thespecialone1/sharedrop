# SharedDrop Environment Configuration
# Copy this file to .env and fill in your values

# Sentry Error Tracking
# Get your DSN from: https://sentry.io/settings/[org]/projects/[project]/keys/
# Main process uses SENTRY_DSN, renderer uses VITE_SENTRY_DSN (same value)
SENTRY_DSN=
VITE_SENTRY_DSN=

# Voice Room TURN Server (optional, improves NAT traversal)
# Self-hosted: use coturn. Paid: Twilio TURN, Xirsys
TURN_URL=
TURN_USER=
TURN_PASS=
